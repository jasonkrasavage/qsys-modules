PluginInfo =
{
    Name = 'Graphic Button',
    Version = '1.6',
    Id = 'pingysssssadfadfadfs',
    Description = 'James Sample Plugin',
    Author = 'James Talmage'
}

local onImgBase64 = ''
local offImgBase64 = ''

function GetProperties()
    return {}
end

function GetControls(props)
    local ctls = {
        {
            Name = 'button_1',
            ControlType = 'Button',
            ButtonType = 'Toggle',
            IconType = 'Image',
            Icon = onImgBase64
        }
    }
    return ctls
end

function GetControlLayout(props)
    local layout  = {
        button_1 = {
            Position={247,52},
            Size={210,180},
            ButtonStyle = "Toggle",
            Color = { 0, 0, 0 },
            Margin = 0,
            Radius = 0,
            Padding = 0,
            CornerRadius = 0,
            Style='Button',
            PrettyName='Sprite'
        }
    }

    local graphics = {}

    return layout, graphics;
end

if Controls then
    local rapidjson = require("rapidjson")

    local function Init()
        local button = Controls['button_1']

        button.EventHandler = function ()
            local img

            if (button.Boolean) then
                img = onImgBase64
            else
                img = offImgBase64
            end

            button.Legend = rapidjson.encode({
                DrawState = true,
                IconData = img
            })
        end

        button:EventHandler()
    end

    Init()
end